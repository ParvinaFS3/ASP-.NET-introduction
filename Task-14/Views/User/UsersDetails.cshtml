@model  List<User>

<div>
    @if (@Model.Count == 0)
    {
        <p>Not Found</p>
    }
    else
    {
        @foreach( var user in Model)
        {
            <div>
                <div>
                    <p>@user.Name</p>
                </div>
            </div>
        }
     
    }
</div>